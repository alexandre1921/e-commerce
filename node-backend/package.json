    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "graphql": "16.7.1",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.22.2",
    "guid-typescript": "1.0.9",
    "reflect-metadata": "0.1.13",
    "tslib": "2.6.0",
    "type-graphql": "2.0.0-beta.2",

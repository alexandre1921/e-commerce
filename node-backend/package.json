{
  "name": "e-commerce-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "generate-types": "graphql-codegen --config codegen.ts",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "db-startup": "npm run migrate && npm run generate",
    "startup": "npm i && npm run db-startup && npm run generate-types",
    "dev": "npm run startup && ts-node-dev --no-notify --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "npx tsc -p './tsconfig.json' || npx tsc-alias"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
